{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-return-await": "error",
    "no-unused-vars": "off",
    "no-useless-escape": "off",
    "object-curly-spacing": ["error", "always"]
  }
}
